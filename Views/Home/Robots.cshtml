
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
<!DOCTYPE HTML>
<html>

<head>
    <meta charset="utf-8">
</head>

<body>

    
    <div>
        <div id="results"></div>
    </div>
    <script>
        $(document).ready(function () {
            GetRobots();
            setInterval(GetRobots, 5000);
        });


        function GetRobots() {

            $.ajax({
                url: "GetRobots",
                contentType: "application/html",

                success: function (result) {
                    $("#results").html(result);
                }
            });
        }

    </script>
</body>

</html>